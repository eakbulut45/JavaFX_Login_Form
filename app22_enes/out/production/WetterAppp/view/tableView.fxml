<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="416.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.161" fx:controller="controller.tableView">
    <children>
        <VBox layoutX="-60.0" layoutY="25.0" prefHeight="214.0" prefWidth="76.0" spacing="23.0">
            <children>
                <Label text="ID">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label text="Name">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label text="Email">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label text="Password">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </VBox>
        <VBox layoutX="15.0" layoutY="25.0" prefHeight="214.0" prefWidth="192.0" spacing="10.0">
            <children>
                <TextField fx:id="tfId" />
                <TextField fx:id="tfName" />
                <TextField fx:id="tfEmail" layoutX="10.0" layoutY="10.0" />
                <TextField fx:id="tfPassword" layoutX="10.0" layoutY="35.0" />
            </children>
        </VBox>
        <TableView fx:id="tvRegister" layoutX="215.0" layoutY="25.0" prefHeight="290.0" prefWidth="425.0">
            <columns>
                <TableColumn fx:id="colId" prefWidth="50.0" text="ID" />
                <TableColumn fx:id="colName" prefWidth="125.0" text="Name" />
                <TableColumn fx:id="colEmail" prefWidth="125.0" text="Email" />
                <TableColumn fx:id="colPassword" prefWidth="125.0" text="Password" />
            </columns>
        </TableView>
        <Button fx:id="btnInsert" layoutX="-60.0" layoutY="279.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="85.0" text="Insert" />
        <Button fx:id="btnUpdate" layoutX="35.0" layoutY="279.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="85.0" text="Update" />
        <Button fx:id="btnDelete" layoutX="130.0" layoutY="279.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="76.0" text="Delete" />
    </children>
</AnchorPane>
